{
  "expo": {
    "name": "barlog",
    "slug": "barlog",
    "description": "Easy preview of barbell plate set",
    "version": "1.3.6",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "barlog",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#2C2C2E",
    "primaryColor": "#FFB703",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFB703",
      "image": "./assets/images/appstore.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.keiver.barlog"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore.png",
        "backgroundColor": "#FFB703"
      },
      "package": "dev.keiver.barlog",
      "splash": {
        "image": "./assets/images/appstore.png",
        "resizeMode": "contain",
        "backgroundColor": "#FFB703"
      },
      "softwareKeyboardLayoutMode": "pan",
      "statusBar": {
        "backgroundColor": "#FFB703",
        "barStyle": "dark-content"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appstore.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "01d0347d-50df-48bf-acf6-2bbd36c9f9d5"
      }
    },
    "owner": "keiverh"
  }
}
