{
  "expo": {
    "name": "barlog",
    "slug": "barlog",
    "description": "Easy preview of barbell plate set",
    "version": "1.4.2",
    "icon": "./assets/icon.png",
    "scheme": "barlog",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#2C2C2E",
    "primaryColor": "#FFB703",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#2C2C2E",
      "image": "./assets/splash-icon.png"
    },
    "ios": {
      "bundleIdentifier": "dev.keiver.barlog"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/playstore.png",
        "backgroundColor": "#FFB703"
      },
      "package": "dev.keiver.barlog",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/appstore.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "01d0347d-50df-48bf-acf6-2bbd36c9f9d5"
      }
    },
    "owner": "keiverh"
  }
}
